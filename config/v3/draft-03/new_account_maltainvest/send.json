{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Create maltainvest account Send",
  "description": "This call opens a new real-money account with the 'maltainvest' Landing Company. This call can be made from a virtual-money account or real-money account at Binary (Europe) Ltd. If itâ€™s the latter, client information fields in this call will be ignored and data from your existing real-money account will be used.",
  "properties": {
    "new_account_maltainvest": {
      "type": "integer",
      "description": "Must be 1",
      "pattern": "^1$",
      "required": "1"
    },
    "salutation" : {
      "enum": ["Mr", "Mrs", "Ms", "Miss"],
      "description": "Accept any value in enum list.",
      "required": "1"
    },
    "first_name": {
      "type": "string",
      "pattern": "^[\\p{L}\\s'.-]{2,30}$",
      "description": "Within 2-30 characters, use only letters, spaces, hyphens, full-stops or apostrophes.",
      "required": "1"
    },
    "last_name": {
      "type": "string",
      "pattern": "^[\\p{L}\\s'.-]{2,30}$",
      "description": "Within 2-30 characters, use only letters, spaces, hyphens, full-stops or apostrophes.",
      "required": "1"
    },
    "date_of_birth" : {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{1,2}?-[0-9]{1,2}?$",
      "description": "Date of birth format: yyyy-mm-dd.",
      "required": "1"
    },
    "citizen": {
      "type" : "string",
      "pattern": "^[a-z]{2}$",
      "description": "Country of legal citizenship, 2-letter country code."
    },
    "residence" : {
      "type" : "string",
      "pattern": "^[a-z]{2}$",
      "description": "2-letter country code, possible value receive from residence_list call.",
      "required": "1"
    },
    "place_of_birth": {
      "type" : "string",
      "pattern": "^[a-z]{2}$",
      "description": "Place of birth, 2-letter country code.",
      "required": "1"
    },
    "address_line_1": {
      "type" : "string",
      "pattern": "^[\\p{L}\\p{Nd}\\s'.,:;()@#\/-]{1,70}$",
      "description": "Within 70 characters.",
      "required": "1"
    },
    "address_line_2": {
      "type" : "string",
      "pattern": "^[\\p{L}\\p{Nd}\\s'.,:;()@#\/-]{0,70}$",
      "description": "Optional field, within 70 characters."
    },
    "address_city": {
      "type" : "string",
      "pattern": "^[\\p{L}\\s'.-]{1,35}$",
      "description": "Within 35 characters",
      "required": "1"
    },
    "address_state": {
      "type" : "string",
      "pattern": "^[\\p{L}\\p{Nd}\\s'.,-]{0,35}$",
      "description": "Optional field, possible value receive from states_list call."
    },
    "address_postcode": {
      "type" : "string",
      "pattern": "^[^+]{0,20}$",
      "description": "Optional field, within 20 characters and may not contain '+'."
    },
    "phone": {
      "type" : "string",
      "pattern": "^\\+?((-|\\s)*[0-9]){8,35}$",
      "description": "Within 8-35 digits, allowing '+' in front, numbers, hyphens or space.",
      "required": "1"
    },
    "secret_question": {
      "enum": [ "Mother's maiden name", "Name of your pet", "Name of first love", "Memorable town/city", "Memorable date", "Favourite dish", "Brand of first car", "Favourite artist" ],
      "description": "Accept any value in enum list."
    },
    "secret_answer": {
      "type": "string",
      "minLength": 4,
      "maxLength": 50,
      "pattern": "^[\\w\\-\\,\\.\\' ]+",
      "description": "Answer to secret question, within 4-50 characters."
    },
    "affiliate_token": {
      "type": "string",
      "pattern": "^[\\w-]{0,32}$",
      "description": "Affiliate token, within 32 characters."
    },
    "tax_residence": {
      "type": "string",
      "pattern": "^[a-z]{0,2}(?:,[a-z]{2})*$",
      "description": "Residence for tax purpose. Comma separated iso country code if multiple jurisdictions. Only applicable for real money account. Required for maltainvest landing company.",
      "required": "1"
    },
    "tax_identification_number": {
      "type": "string",
      "pattern": "^[\\w-]{0,20}$",
      "description": "Tax identification number. Only applicable for real money account. Required for maltainvest landing company.",
      "required": "1"
    },
    "forex_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Forex trading experience"
    },
    "forex_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Forex trading frequency"
    },
    "binary_options_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Binary options trading experience"
    },
    "binary_options_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Binary options trading frequency"
    },
    "cfd_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "CFDs trading experience"
    },
    "cfd_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "CFDs trading frequency"
    },
    "other_instruments_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Trading experience in other financial instruments"
    },
    "other_instruments_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Trading frequency in other financial instruments"
    },
    "account_opening_reason": {
      "type": "string",
      "description": "Purpose and reason for requesting the account opening",
      "enum": [
        "Speculative",
        "Income Earning",
        "Hedging"
      ]
    },
    "account_turnover": {
      "enum": [ "Less than $25,000", "$25,000 - $50,000", "$50,001 - $100,000", "$100,001 - $500,000", "Over $500,000" ],
      "description": "The anticipated account turnover"
    },
    "employment_industry": {
      "enum": [ "Construction", "Education", "Finance", "Health", "Tourism", "Information & Communications Technology", "Science & Engineering", "Legal", "Social & Cultural", "Agriculture", "Real Estate", "Food Services", "Manufacturing", "Unemployed"],
      "description": "Industry of Employment",
      "required": 1
    },
    "education_level": {
      "enum": [ "Primary", "Secondary", "Tertiary" ],
      "description": "Level of Education",
      "required": 1
    },
    "income_source": {
      "enum": [ "Salaried Employee", "Self-Employed", "Investments & Dividends", "Pension", "State Benefits", "Savings & Inheritance"],
      "description": "Income Source",
      "required": 1
    },
    "net_income": {
      "enum": [ "Less than $25,000", "$25,000 - $50,000", "$50,001 - $100,000", "$100,001 - $500,000", "Over $500,000" ],
      "description": "Net Annual Income",
      "required": 1
    },
    "estimated_worth": {
      "enum": [ "Less than $100,000", "$100,000 - $250,000", "$250,001 - $500,000", "$500,001 - $1,000,000", "Over $1,000,000" ],
      "description": "Estimated Net Worth",
      "required": 1
    },
    "occupation": {
      "enum": ["Chief Executives, Senior Officials and Legislators", "Managers", "Professionals", "Clerks", "Personal Care, Sales and Service Workers", "Agricultural, Forestry and Fishery Workers", "Craft, Metal, Electrical and Electronics Workers", "Plant and Machine Operators and Assemblers", "Cleaners and Helpers", "Mining, Construction, Manufacturing and Transport Workers", "Armed Forces", "Government Officers", "Students", "Unemployed"],
      "description": "Occupation",
      "required": "1"
    },
    "employment_status" : {
      "enum": ["Employed", "Pensioner", "Self-Employed", "Student", "Unemployed"],
      "description": "Employment Status",
      "required": 0
    },
    "source_of_wealth" : {
      "enum": ["Accumulation of Income/Savings", "Cash Business", "Company Ownership", "Divorce Settlement", "Inheritance", "Investment Income", "Sale of Property"],
      "description": "Source of wealth",
      "required": 0
    },
    "accept_risk" : {
      "type": "integer",
      "description": "Show whether client has accepted risk disclaimer, boolean value 1 or 0",
      "required": 1
    },
    "client_type": {
      "pattern": "^retail|professional$",
      "required": "0",
      "description": "Optional field, indicates whether this is for a client requesting an account with professional status. Defaults to retail."
    },
    "passthrough": {
      "type": "object",
      "description": "Optional field, used to pass data through the websocket, which may be retrieved via the echo_req output field."
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field to map request to response"
    }
  },
  "additionalProperties": false
}

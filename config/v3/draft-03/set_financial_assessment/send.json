{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Save Financial assessment details send",
  "description": "This call sets the financial assessement details based on the client's answers to analyse whether he possess the experience and knowledge to understand the risks involved with binary options trading.",
  "properties": {
    "set_financial_assessment": {
      "type": "integer",
      "description": "Must be 1",
      "pattern": "^1$",
      "required": "1"
    },
    "forex_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Forex trading experience",
      "required": "0"
    },
    "forex_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Forex trading frequency",
      "required": "0"
    },
    "binary_options_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Binary options trading experience",
      "required": "0"
    },
    "binary_options_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Binary options trading frequency",
      "required": "0"
    },
    "cfd_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "CFDs trading experience",
      "required": "0"
    },
    "cfd_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "CFDs trading frequency",
      "required": "0"
    },
    "other_instruments_trading_experience": {
      "enum": [ "0-1 year", "1-2 years", "Over 3 years" ],
      "description": "Trading experience in other financial instruments",
      "required": "0"
    },
    "other_instruments_trading_frequency": {
      "enum": [ "0-5 transactions in the past 12 months", "6-10 transactions in the past 12 months", "11-39 transactions in the past 12 months", "40 transactions or more in the past 12 months" ],
      "description": "Trading frequency in other financial instruments",
      "required": "0"
    },
    "employment_industry": {
      "enum": [ "Construction", "Education", "Finance", "Health", "Tourism", "Information & Communications Technology", "Science & Engineering", "Legal", "Social & Cultural", "Agriculture", "Real Estate", "Food Services", "Manufacturing", "Unemployed"],
      "description": "Industry of Employment",
      "required": "1"
    },
    "education_level": {
      "enum": [ "Primary", "Secondary", "Tertiary" ],
      "description": "Level of Education",
      "required": "1"
    },
    "income_source": {
      "enum": [ "Salaried Employee", "Self-Employed", "Investments & Dividends", "Pension","State Benefits", "Savings & Inheritance"],
      "description": "Income Source",
      "required": "1"
    },
    "net_income": {
      "enum": [ "Less than $25,000", "$25,000 - $50,000", "$50,001 - $100,000", "$100,001 - $500,000", "Over $500,000" ],
      "description": "Net Annual Income",
      "required": "1"
    },
    "estimated_worth": {
      "enum": [ "Less than $100,000", "$100,000 - $250,000", "$250,001 - $500,000", "$500,001 - $1,000,000", "Over $1,000,000" ],
      "description": "Estimated Net Worth",
      "required": "1"
    },
    "account_turnover": {
      "enum": [ "Less than $25,000", "$25,000 - $50,000", "$50,001 - $100,000", "$100,001 - $500,000", "Over $500,000" ],
      "description": "The anticipated account turnover"
    },
    "occupation": {
      "enum": ["Chief Executives, Senior Officials and Legislators", "Managers", "Professionals", "Clerks", "Personal Care, Sales and Service Workers", "Agricultural, Forestry and Fishery Workers", "Craft, Metal, Electrical and Electronics Workers", "Plant and Machine Operators and Assemblers", "Cleaners and Helpers", "Mining, Construction, Manufacturing and Transport Workers", "Armed Forces", "Government Officers", "Students", "Unemployed"],
      "description": "Occupation",
      "required": "1"
    },
    "employment_status" : {
      "enum": ["Employed", "Pensioner", "Self-Employed", "Student", "Unemployed"],
      "description": "Employment Status",
      "required": 0
    },
    "source_of_wealth" : {
      "enum": ["Accumulation of Income/Savings", "Cash Business", "Company Ownership", "Divorce Settlement", "Inheritance", "Investment Income", "Sale of Property"],
      "description": "Source of wealth",
      "required": 0
    },
    "passthrough": {
      "type": "object",
      "description": "Optional field, used to pass data through the websocket, which may be retrieved via the echo_req output field."
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field to map request to response"
    }
  },
  "additionalProperties": false
}
